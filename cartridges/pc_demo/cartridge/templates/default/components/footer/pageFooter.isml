                </div>
            </div>
            <div class="row mt-3">
                <div class="col-4 black-text">
                    <p class="white-text no-marginy-no-paddingy">Siguenos</p>
                </div>
                <div class="col-8 black-text -marginy-no-paddingy">
                    <p class="white-text no-marginy-no-paddingy">Pagos Seguros</p>
                </div>
            </div>
            <div class="row">
                <div class="col-4">
                    <div class="btn-group" role="group" aria-label="Basic example">
                        <div class="minicart-total hide-link-med">
                            <a class="minicart-link" href="${URLUtils.https('Cart-Show')}" title="${Resource.msgf('minicart.count','common',null, pdict.quantityTotal)}" aria-label="${Resource.msgf('minicart.count','common',null, pdict.quantityTotal)}" aria-haspopup="true">
                                <i class="minicart-icon fa fa-facebook-official white-text"></i>
                                <span class="minicart-quantity">
                                    <isprint value="${pdict.quantityTotal}" formatter="##" />
                                </span>
                            </a>
                        </div>
                        <div class="mx-2">
                            <div class="minicart-total hide-link-med">
                                <a class="minicart-link" href="${URLUtils.https('Cart-Show')}" title="${Resource.msgf('minicart.count','common',null, pdict.quantityTotal)}" aria-label="${Resource.msgf('minicart.count','common',null, pdict.quantityTotal)}" aria-haspopup="true">
                                    <i class="minicart-icon fa fa-instagram white-text"></i>
                                    <span class="minicart-quantity">
                                        <isprint value="${pdict.quantityTotal}" formatter="##" />
                                    </span>
                                </a>
                            </div>
                        </div>
                        <div class="minicart-total hide-link-med">
