<footer class="bg-blue-2 no-marginy-no-paddingy" id="footercontent">
    <div class="footer-container row-1" style="padding-top: 100px; padding-left:80px; padding-right:80px">
        <div class="footer-item col-sm-12 col-md-12 col-lg-2 center-items">
            <iscontentasset aid="footer-terms-and-conditions" />
        </div>
        <div class="footer-item col-sm-12 col-md-12 col-lg-2 center-items">
            <iscontentasset aid="footer-website" />
        </div>
        <div class="footer-item col-sm-12 col-md-12 col-lg-2 center-items">
            <iscontentasset aid="footer-company-information" />
        </div>
        <div class="footer-item col-sm-12 col-md-12 col-lg-4">
