                                <i class="minicart-icon fa fa-cc-paypal white-text"></i>
                                <span class="minicart-quantity">
                                    <isprint value="${pdict.quantityTotal}" formatter="##" />
                                </span>
                            </a>
                        </div>
                        <div class="minicart-total hide-link-med mr-2">
                            <a class="minicart-link" href="${URLUtils.https('Cart-Show')}" title="${Resource.msgf('minicart.count','common',null, pdict.quantityTotal)}" aria-label="${Resource.msgf('minicart.count','common',null, pdict.quantityTotal)}" aria-haspopup="true">
                                <i class="minicart-icon fa fa-cc-discover white-text"></i>
                                <span class="minicart-quantity">
                                    <isprint value="${pdict.quantityTotal}" formatter="##" />
                                </span>
                            </a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-sm-12 col-md-12 col-lg-2 center-items">
            <a href="${URLUtils.url('Home-Show')}" title="${ Resource.msgf('global.homepage.tooltip', 'common', null, Resource.msg('global.storename', 'common', null), Resource.msg('global.home', 'common', null)) }">
                <img class="footer-logo" src="${URLUtils.staticURL('/images/logo.svg')}" alt="${Resource.msg('global.storename', 'common', null)}" />
            </a>
        </div>
    </div>
    <iscomment>
    <hr class="hidden-xs-down" />
    </iscomment>
    <div class="row-1 bg-light">
    <iscomment>
        <div class="col-lg-4 col-sm-5 push-sm-7 push-lg-8 social">
            <isslot id="footer-column-m" description="Content in column 1 of the Footer" context="global" />
            <button class="back-to-top" title="${Resource.msg('button.back_to_top', 'common', null)}" aria-label="${Resource.msg('button.back_to_top', 'common', null)}">
                <span class="fa-stack fa-lg" aria-hidden="true">
                    <i class="fa fa-circle fa-inverse fa-stack-2x"></i>
                    <i class="fa fa-arrow-up fa-stack-1x"></i>
                </span>
            </button>
        </div>
    </iscomment>    
