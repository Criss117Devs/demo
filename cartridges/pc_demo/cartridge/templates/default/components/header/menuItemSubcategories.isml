<ul class="ul-vertical bg-white-1" role="menu" aria-hidden="true" aria-label="${category.id}">
    <isloop items="${menuItem.subCategories}" var="category">
        <isif condition="${category.subCategories}">
        <iselse/>
            <li class="" role="presentation">
                <a href="${category.url}" id="${category.id}" role="menuitem" class="" tabindex="0">${category.name}</a>
            </li>
        </isif>
    </isloop>
</ul>